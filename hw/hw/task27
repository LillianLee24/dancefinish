}


public class task3518 {
    public static void main(String[] args) {
        int[] numbers = {10, 20, 30, 40, 50};
        int target = 10;
        int index = -1;
        for (int i = 0; i < numbers.length; i++) {
            if (numbers[i] == target) {
                index = i;
                break;
            }
        }
        if (index != -1) {
            System.out.println(index);
        } else {
            System.out.println("not found");
        }
    }
}


