import java.util.Scanner;
public class task354 {
    public static void main(String[]arg){
        Scanner scanner = new Scanner(System.in);
        System.out.println("enter a num: ");
        int divident = scanner.nextInt();
        System.out.println("enter a num: ");
        int divisor = scanner.nextInt();
        int remainder = divident % divisor;
        System.out.println("the remainder is: "+ remainder);
        scanner.close();
    }
}


import java.util.Scanner;
public class task355 {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        System.out.println("Проверьте, подключен ли ваш компьютер к интернету (да/нет): ");
        String response = scanner.nextLine();
        if (response.equalsIgnoreCase("да")) {
            System.out.println("Интернет подключен");
        } else {
            System.out.println("Нет подключения к интернету");
        }
        scanner.close();
    }
}


