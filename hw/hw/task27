import java.util.Arrays;
public class task3520 {
    public static void main(String[] args) {
        int[] array = {1, 2, 3, 4, 5};
        System.out.println("first version: " + Arrays.toString(array));
        if (array.length >= 2) {
            int temp = array[0];
            array[0] = array[array.length - 1];
            array[array.length - 1] = temp;
        }
        System.out.println("second version: " + Arrays.toString(array));
    }
}


